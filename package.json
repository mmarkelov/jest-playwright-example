  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mmarkelov/jest-playwright-example.git"
  },
  "keywords": [
    "jest",
    "playwright",
    "jest-playwright",
    "example"
  ],
  "author": "mmarkelov",
  "bugs": {
    "url": "https://github.com/mmarkelov/jest-playwright-example/issues"
  },
  "homepage": "https://github.com/mmarkelov/jest-playwright-example#readme",
  "devDependencies": {
    "jest": "^25.1.0",
    "playwright": "^0.9.24",
    "rimraf": "^3.0.1"
  }
}
